<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="bda.plone.shop">

  <records prefix="plone.resources/cookie-functions"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'>
    <value key="js">++resource++bda.plone.shop.cookie.functions.js</value>
  </records>

  <records prefix="plone.resources/bda-plone-shop"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'>
    <value key="js">++resource++bda.plone.shop.js</value>
    <value key="css">
      <element>++resource++bda.plone.shop.less</element>
    </value>
  </records>

  <records prefix="plone.bundles/bda-plone-shop-bundle"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>cookie-functions</element>
      <element>bda-plone-shop</element>
    </value>
    <value key="enabled">True</value>
    <value key="compile">True</value>
    <value key="jscompilation">++plone++bda.plone.shop.compiled/shop.css</value>
    <value key="csscompilation">++plone++bda.plone.shop.compiled/shop.js</value>
    <value key="last_compilation">2016-03-04 17:00:00</value>
   </records>

</registry>
